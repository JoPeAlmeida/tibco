<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <pd:name>broadworksReporting/ReadParseXmlCData.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>41</pd:startX>
    <pd:startY>165</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>1134</pd:endX>
    <pd:endY>163</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <multXmlString>
            <xsd:element name="root" type="xsd:string"/>
        </multXmlString>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1491481712106</pd:targetNamespace>
    <pd:activity name="Parse Data">
        <pd:type>com.tibco.plugin.parse.ParseActivity</pd:type>
        <pd:resourceType>ae.activities.ParseActivity</pd:resourceType>
        <pd:x>154</pd:x>
        <pd:y>162</pd:y>
        <config>
            <InputType>File</InputType>
            <Encoding>ASCII</Encoding>
            <ParseSharedConfig>/broadworksReporting/Data Format.sharedparse</ParseSharedConfig>
        </config>
        <pd:inputBindings>
            <Input>
                <fileName>
                    <xsl:value-of select="&quot;C:\Users\jpa\Downloads\tibco\rr.log.2017-04-05&quot;"/>
                </fileName>
                <noOfRecords>
                    <xsl:value-of select="48795"/>
                </noOfRecords>
            </Input>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>254</pd:x>
        <pd:y>1</pd:y>
        <pd:width>737</pd:width>
        <pd:height>310</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$Parse-Data/Output/Rows/root/param</pd:over>
            <pd:iterationElementSlot>each2line</pd:iterationElementSlot>
            <pd:indexSlot>index</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Null</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-65485</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>ConcatVariable</pd:to>
            <pd:xpathDescription>elseIF</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-13421569</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>contains($each2line/param, "&lt;")</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Transform XML</pd:to>
            <pd:xpathDescription>OK</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16738048</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>not(contains($each2line/param, "&lt;")) and (string-length($multXmlString/root) &gt; 0) 
and (string-length(substring-before(substring-after($multXmlString/root, "&lt;![CDATA["), "]]&gt;"))&gt;0)</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>Null</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>ClearVariable</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>ConcatVariable</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Transform XML</pd:from>
            <pd:to>Write File</pd:to>
            <pd:xpathDescription>isNotEmpty</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-13395712</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>contains(substring-after($Transform-XML/xmlString, "?&gt;"), "&lt;")</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>Write File</pd:from>
            <pd:to>ClearVariable</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Transform XML</pd:from>
            <pd:to>ClearVariable</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-65536</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:activity name="Transform XML">
            <pd:type>com.tibco.plugin.xml.XMLTransformActivity</pd:type>
            <pd:resourceType>ae.activities.XMLTransformActivity</pd:resourceType>
            <pd:x>470</pd:x>
            <pd:y>105</pd:y>
            <config>
                <inputStyle>text</inputStyle>
                <engine>TibcoXML</engine>
                <stylesheet>/broadworksReporting/OCI2CORE_R20.xsl</stylesheet>
            </config>
            <pd:inputBindings>
                <textInput>
                    <xmlString>
                        <xsl:value-of select="substring-before(substring-after($multXmlString/root, &quot;&lt;![CDATA[&quot;), &quot;]]>&quot;)"/>
                    </xmlString>
                </textInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Write File">
            <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
            <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
            <pd:x>641</pd:x>
            <pd:y>56</pd:y>
            <config>
                <encoding>text</encoding>
                <compressFile>None</compressFile>
                <append>true</append>
            </config>
            <pd:inputBindings>
                <ns0:WriteActivityInputTextClass>
                    <fileName>
                        <xsl:value-of select="concat(&quot;C:\Users\jpa\Downloads\tibco\&quot;,&quot;File.txt&quot;)"/>
                    </fileName>
                    <textContent>
                        <xsl:value-of select="concat($Transform-XML/xmlString,&quot;&#xA;&quot;,  &quot;&lt;!--                    ANOTHER ONE: RECORD No &quot;,$index , &quot;                   -->&quot;)"/>
                    </textContent>
                    <addLineSeparator>
                        <xsl:value-of select="true()"/>
                    </addLineSeparator>
                </ns0:WriteActivityInputTextClass>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="ConcatVariable">
            <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
            <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
            <pd:x>631</pd:x>
            <pd:y>167</pd:y>
            <config>
                <variableName>multXmlString</variableName>
            </config>
            <pd:inputBindings>
                <root>
                    <xsl:value-of select="concat($multXmlString/root, concat(&quot;&lt;&quot;,substring-after($each2line/param, &quot;&lt;&quot;)))"/>
                </root>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="ClearVariable">
            <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
            <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
            <pd:x>813</pd:x>
            <pd:y>106</pd:y>
            <config>
                <variableName>multXmlString</variableName>
            </config>
            <pd:inputBindings>
                <root>
                    <xsl:value-of select="&quot;&quot;"/>
                </root>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Null">
            <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
            <pd:resourceType>ae.activities.null</pd:resourceType>
            <pd:x>628</pd:x>
            <pd:y>245</pd:y>
            <config/>
            <pd:inputBindings/>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Parse Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse Data</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>